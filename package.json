		"vuepress-plugin-container": "^2.1.5",
		"vuepress-plugin-feed": "^0.1.9",
		"vuepress-plugin-minimal-analytics": "^0.1.4",
		"vuepress-plugin-reading-progress": "^1.0.10",
		"vuepress-plugin-reading-time": "^0.1.1",
		"vuepress-plugin-seo": "^0.2.0",
