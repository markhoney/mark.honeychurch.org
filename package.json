		"markdown-it-footnote": "^3.0.3",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-video": "^0.6.3",
		"markdown-pdf": "^10.0.0",
		"pdfvuer": "^1.9.2",
		"seespee": "^3.0.0",
		"vitepress": "^0.22.3",
		"vue": "^2.6.14",
		"vue-google-charts": "^0.3.3",
		"vue-head": "^2.2.0",
		"vue-meta": "^2.4.0",
		"vue-parallaxy": "^1.1.1",
