	"scripts": {
		"csp": "seespee --root ./docs/.vuepress/dist/ ./docs/.vuepress/dist/media/index.html > ./docs/.vuepress/website/csp.txt",
		"favicon": "real-favicon generate ./docs/.vuepress/website/faviconDescription.json ./docs/.vuepress/public/media/images/icons/faviconData.json ./docs/.vuepress/public/media/images/icons",
		"cv": "node ./docs/cv/pdf.js",
		"dev": "vitepress dev docs",
		"build": "vitepress build docs",
		"serve": "vitepress serve docs",
		"kill": "taskkill /IM node.exe /F",
		"up": "yarn upgrade && yarn upgrade-interactive --latest"
	},
	"directories": {
		"doc": "docs"
	},
