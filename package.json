		"cv": "node ./docs/cv/pdf.js",
		"dev": "vuepress dev docs",
		"build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vuepress build docs",
		"serve": "vuepress serve docs",
		"kill": "taskkill /IM node.exe /F",
		"ug": "yarn upgrade && yarn upgrade-interactive --latest"
	},
	"directories": {
		"doc": "docs"
	},
