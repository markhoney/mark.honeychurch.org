		"@vue/component-compiler-utils": "^3.3.0",
		"@vuepress/plugin-blog": "^1.9.4",
		"@vuepress/plugin-google-analytics": "^1.9.5",
		"@vuepress/plugin-medium-zoom": "^1.9.5",
		"@vuepress/plugin-pwa": "^1.9.5",
		"@vuepress/theme-blog": "^2.3.3",
		"@vuepress/theme-default": "^1.9.5",
		"add": "^2.0.6",
		"axios": "^0.26.0",
		"buefy": "^0.9.14",
		"cheerio": "^1.0.0-rc.10",
		"cli-real-favicon": "^0.0.8",
		"cross-env": "^7.0.3",
		"csv": "^5.5.0",
		"favicons": "^7.0.0-beta.3",
		"gridsome": "^0.7.23",
		"html-entities": "^1.4.0",
		"jshint": "^2.13.3",
