{
	"name": "mark.honeychurch.org",
	"version": "1.0.0",
	"description": "Mark Honeychurch's Personal Website",
	"author": "Mark Honeychurch",
	"license": "ISC",
	"homepage": "https://mark.honeychurch.org",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/markhoney/mark.honeychurch.org.git"
	},
	"scripts": {
		"csp": "seespee --root ./dist/ ./dist/media/index.html > ./website/csp.txt",
		"favicon": "real-favicon generate ./website/faviconDescription.json ./public/media/images/icons/faviconData.json ./public/media/images/icons",
		"cv": "node ./docs/cv/pdf.js",
		"dev": "yarn dev:vuepress",
		"dev:vuepress": "node ./node_modules/vuepress/cli.js dev -c ./docs/.vuepress/config.js ./docs",
		"dev:vuepress:next": "node ./node_modules/vuepress-next/bin/vuepress.js dev docs -c ./.vuepress-next/config.js",
		"dev:vuepress:vite": "node ./node_modules/vuepress-vite/bin/vuepress.js dev docs -c ./.vuepress-next/config.vite.js",
		"dev:vitepress": "vitepress dev docs",
		"dev:gridsome": "gridsome develop",
		"dev:gridsome:info": "gridsome info",
		"build": "yarn build:vuepress",
		"build:vuepress": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vuepress build docs",
		"serve": "vuepress serve docs",
		"kill": "taskkill /IM node.exe /F",
		"upgrade:major": "yarn upgrade-interactive --latest"
	},
	"directories": {
		"doc": "docs"
	},
	"devDependencies": {
		"@gridsome/cli": "^0.3.4",
		"@gridsome/source-filesystem": "^0.6.2",
		"@gridsome/transformer-remark": "^0.6.4",
